        <name>hbase.client.pause</name>
        <value>100</value>
    </property>
    <!-- 主节点信息绑定地址 -->
    <property>
        <name>hbase.master.info.bindAddress</name>
        <value>xlhd-b3001-cn02</value>
    </property>
    <!-- RMI注册表端口 -->
    <property>
        <name>master.rmi.registry.port</name>
        <value>21306</value>
    </property>
    <!-- 短路本地读取 -->
    <property>
        <name>dfs.client.read.shortcircuit</name>
        <value>true</value>
    </property>
    <!-- 是否预加载表描述符 -->
    <property>
        <name>hbase.master.preload.tabledescriptors</name>
        <value>false</value>
    </property>
    <!-- phoenix 协处理器 元数据缓存的最大存活时间（毫秒） -->
    <property>
        <name>phoenix.coprocessor.maxMetaDataCacheTimeToLiveMs</name>
        <value>1800000</value>
    </property>
    <!-- 读请求在CallQueue中所占的比例 -->
    <property>
        <name>hbase.ipc.server.callqueue.read.ratio</name>
        <value>0</value>
    </property>
    <!-- 主节点过程线程数 -->
    <property>
        <name>hbase.master.procedure.threads</name>
        <value>20</value>
    </property>
    <!-- HFile大型清理线程数 -->
    <property>
        <name>hbase.regionserver.hfilecleaner.large.thread.count</name>
        <value>1</value>
    </property>
    <!-- 批量分配线程池大小 -->
    <property>
        <name>hbase.bulk.assignment.threadpool.size</name>
        <value>20</value>
    </property>
    <!-- 主节点DNS接口 -->
    <property>
        <name>hbase.master.dns.interface</name>
        <value>default</value>
    </property>
    <!-- 等待启动的区域服务器最小数目 -->
    <property>
        <name>hbase.master.wait.on.regionservers.mintostart</name>
        <value>1</value>
    </property>
    <!-- 是否启用区域双分配自动恢复 -->
    <property>
        <name>hbase.region.double.assignment.auto.recovery.enabled</name>
        <value>true</value>
    </property>
    <!-- 主节点ACL初始化超时时间（毫秒） -->
    <property>
        <name>hbase.master.acl.init.timeout</name>
        <value>300000</value>
    </property>
    <!-- 客户端KeyValue的最大大小 -->
    <property>
        <name>hbase.client.keyvalue.maxsize</name>
        <value>10485760</value>
    </property>
    <!-- 是否进行HBase权限检查 -->
    <property>
        <name>hbase.security.exec.permission.checks</name>
        <value>true</value>
    </property>
    <!-- 客户端Scanner的缓存大小 -->
    <property>
        <name>hbase.client.scanner.caching</name>
        <value>100</value>
    </property>
    <!-- HLog的读取器实现类 -->
    <property>
        <name>hbase.regionserver.hlog.reader.impl</name>
        <value>org.apache.hadoop.hbase.regionserver.wal.SecureProtobufLogReader</value>
    </property>
    <!-- RPC超时时间（毫秒） -->
    <property>
        <name>hbase.rpc.timeout</name>
        <value>60000000</value>
    </property>
    <!-- ZooKeeper客户端端口 -->
    <property>
        <name>hbase.zookeeper.property.clientPort</name>
        <value>24002</value>
    </property>
    <!-- 是否启用WAL（Write-Ahead Log）加密 -->
    <property>
        <name>hbase.regionserver.wal.encryption</name>
        <value>false</value>
    </property>
    <!-- 是否启用复制批量加载 -->
    <property>
        <name>hbase.replication.bulkload.enabled</name>
        <value>false</value>
    </property>
    <!-- 是否启用区域自动恢复 -->
    <property>
        <name>hbase.region.assignment.auto.recovery.enabled</name>
        <value>true</value>
    </property>
    <!-- 是否启用文件流清理器TTL -->
    <property>
        <name>hbase.fileStream.cleaner.ttl.enable</name>
        <value>false</value>
    </property>
    <!--RPC闲置时间-->
    <property>
        <name>hbase.ipc.client.connection.minIdleTimeBeforeClose</name>
        <value>3600000</value>
    </property>
</configuration>
